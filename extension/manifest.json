{
  "name": "DHT Comments",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Comment on websites and share them",
  "browser_action": {
  },
  "permissions": [
    "http://*/*", "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["comments.js"],
      "run_at": "document_end"
    }
  ]
}
